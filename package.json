{
  "name": "lean-tag",
  "version": "1.0.0",
  "description": "Utility for creating HTML elements",
  "keywords": [
    "hyperscript",
    "html element"
  ],
  "bugs": {
    "url": "https://github.com/autioch/lean-tag/issues"
  },
  "author": "autioch@gmail.com",
  "license": "ISC",
  "main": "dist/lean-tag.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/autioch/lean-tag.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "webpack --config tools/webpack.config.lib.js -p --hide-modules",
    "docs:watch": "webpack --config tools/webpack.config.docs.js --watch --hide-modules",
    "docs:production": "webpack --config tools/webpack.config.docs.js -p --hide-modules",
    "lint": "eslint ./index.js --fix --ext .js & exit 0"
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-loader": "6.2.9",
    "babel-preset-es2015": "6.18.0",
    "clean-webpack-plugin": "0.1.14",
    "eslint": "3.17.1",
    "eslint-config-qb": "1.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.24.1",
    "webpack": "1.14.0",
    "webpack-validator": "2.2.12",
    "webpack-livereload-plugin": "0.9.0"
  }
}
